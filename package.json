{
  "name": "TrackTik-Test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "serve:docker": "yarn serve --mode docker",
    "build": "yarn (vue-cli-service lint) && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "lint": "vue-cli-service lint",
    "dev": "(vue-cli-service lint) && (vue-cli-service serve --open)",
    "inspect": "vue-cli-service inspect",
    "install:clean": "rm -rf node_modules/ && yarn install && yarn dev",
    "lint:fix": "vue-cli-service lint --fix",
    "report": "vue-cli-service build --report",
    "report-json": "vue-cli-service build --report-json",
    "serve:wsl": "vue-cli-service serve",
    "updateSnapshots": "jest -u",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build"
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@fortawesome/fontawesome-free": "^5.7.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "@mdi/font": "^5.4.55",
    "animate.css": "^4.1.1",
    "axios": "^0.19.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.5.3",
    "core-js": "^3.6.2",
    "deepmerge": "^4.2.2",
    "fstream": "^1.0.12",
    "jquery": "^3.3.1",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "popper.js": "^1.14.7",
    "qs": "^6.9.1",
    "register-service-worker": "^1.6.2",
    "roboto-fontface": "^0.10.0",
    "vue": "^2.6.11",
    "vue-meta": "^2.3.1",
    "vue-router": "^3.4.9",
    "vue-toasted": "^1.1.27",
    "vue2-transitions": "^0.3.0",
    "vuedraggable": "^2.23.2",
    "vuetify": "^2.3.6",
    "vuex": "^3.6.0",
    "vuex-map-fields": "^1.3.5",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.8.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.158",
    "@types/vuedraggable": "^2.23.1",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.15.0",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-pwa": "^4.1.2",
    "@vue/cli-plugin-router": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-plugin-vuex": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.2",
    "babel-eslint": "^10.0.1",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "flush-promises": "^1.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "jest-serializer-vue": "^2.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.0",
    "sinon": "^9.0.2",
    "ts-jest": "^25.5.1",
    "typescript": "3.8.3",
    "unzipper": "^0.10.5",
    "vue-cli-plugin-styleguidist": "^4.22.0",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-cli-plugin-webpack-bundle-analyzer": "^2.0.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.6.0",
    "webpack": "^4.41.5"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testURL": "http://localhost/",
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
